<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onboarding">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo_ap"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_logo_ap_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Onboarding">
        <activity android:name=".SplashActivity"
            android:theme="@style/Theme.Onboarding.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SplashActivity2"
            android:theme="@style/Theme.Onboarding.NoActionBar"/>
        <activity
            android:name=".PaginaInicioActivity"
            android:theme="@style/Theme.PaginaInicial" />
        <activity android:name=".MainActivity"
            android:theme="@style/Theme.Onboarding.NoActionBar"/>
        <activity android:name=".AudifonosActivity"/>
        <activity android:name=".MonitoresActivity"/>
        <activity android:name=".ProcesadoresActivity"/>
        <activity android:name=".TarjetasActivity"/>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>