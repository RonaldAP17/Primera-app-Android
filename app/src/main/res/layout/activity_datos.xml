<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background_app"
    android:orientation="vertical">

    <ImageButton
        android:id="@+id/imgbtnTarjetas"
        android:layout_width="179dp"
        android:layout_height="174dp"
        android:layout_marginBottom="83dp"
        android:background="@drawable/buttons_shape"
        android:contentDescription="@string/imagen_de_tarjetas_graficas"
        android:onClick="verPaginaWebGratuito"
        android:src="@drawable/dominio"
        app:layout_constraintBottom_toTopOf="@+id/imgbtnAudifonos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="24dp"
        android:fontFamily="@font/telex"
        android:text="Sitio web propio"
        android:textColor="#151515"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imgbtnAudifonos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgbtnTarjetas"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="24dp"
        android:fontFamily="@font/telex"
        android:text="Sitio web gratuito"
        android:textColor="#151515"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/imgbtnTarjetas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <ImageButton
        android:id="@+id/imgbtnAudifonos"
        android:layout_width="179dp"
        android:layout_height="174dp"
        android:layout_marginTop="344dp"
        android:layout_marginBottom="56dp"
        android:background="@drawable/buttons_shape"
        android:contentDescription="@string/imagen_de_audifonos"
        android:onClick="verPaginaWebPropia"
        android:src="@drawable/educacion_en_linea"
        app:layout_constraintBottom_toTopOf="@+id/view"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="94dp"
        android:background="@drawable/buttons_shape"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/imgBtnInicio"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginEnd="2dp"
        android:background="@color/white"
        android:onClick="verPaginaInicio"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toStartOf="@+id/imgbtnCelulares"
        app:layout_constraintHorizontal_bias="0.791"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view"
        android:src="@drawable/ordenador_portatil"
        />

    <ImageButton
        android:id="@+id/imgbtnCelulares"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@color/white"
        android:onClick="verPaginaCelulares"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintStart_toStartOf="@+id/view"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:srcCompat="@drawable/telefono_inteligente" />

    <ImageButton
        android:id="@+id/imgbtndatos"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:background="@color/white"
        android:onClick="verPaginaDatos"
        app:layout_constraintBottom_toBottomOf="@+id/view"
        app:layout_constraintEnd_toEndOf="@+id/view"
        app:layout_constraintHorizontal_bias="0.217"
        app:layout_constraintStart_toEndOf="@+id/imgbtnCelulares"
        app:layout_constraintTop_toTopOf="@+id/view"
        app:srcCompat="@drawable/usuario" />

    <ImageButton
        android:id="@+id/imgbtndatos2"
        android:layout_width="64dp"
        android:layout_height="65dp"
        android:background="@color/background_app"
        android:onClick="verInfoServicio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/informacion" />


</androidx.constraintlayout.widget.ConstraintLayout>